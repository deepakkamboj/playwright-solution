{
  "name": "@ppux/playwright-solution",
  "description": "Integration Test Framework based on Playwright and Jest",
  "version": "1.0.0",
  "main": "./lib/index.js",
  "private": true,
  "license": "ISC",
  "scripts": {
    "run-tests": "npx ts-node ./src/integration-tests.ts",
    "record": "npx playwright codegen",
    "test": "node ./lib/integration-tests.js",
    "build": "node ./node_modules/typescript/bin/tsc -p ./tsconfig.json",
    "prettier": "prettier --check **/*{.ts,.tsx,.js,.json}",
    "prettier:fix": "prettier --write **/*{.ts,.tsx,.js,.json}"
  },
  "dependencies": {
    "@jest/test-result": "26.3.0",
    "@jest/types": "26.3.0",
    "@types/fs-extra": "8.1.0",
    "@types/jest": "^26.0.20",
    "@types/node": "12.12.31",
    "eslint": "7.31.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-jest": "24.1.3",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "jest-playwright-preset": "1.4.5",
    "jest-trx-results-processor": "2.2.0",
    "fs-extra": "8.1.0",
    "jest-runner-groups": "2.1.0",
    "playwright": "^1.12.3",
    "playwright-core": "1.12.3",
    "prettier": "2.3.2",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "devDependencies": {}
}
